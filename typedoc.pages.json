{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/index.ts",
    "src/products.ts",
    "src/collections.ts",
    "src/checkout.ts",
    "src/store.ts"
  ],
  "out": "docs",
  "theme": "default",
  "tsconfig": "tsconfig.json",
  "exclude": ["**/*.test.ts", "examples/**"],
  "excludeExternals": false,
  "excludePrivate": false,
  "excludeProtected": false,
  "searchInComments": true,
  "categorizeByGroup": false
}
